  "version": "1.0.0-*",

  "dependencies": {
    "Constants": "1.0.0-*",
    "Microsoft.AspNet.Mvc": "6.0.0-beta8-15875",
    "Microsoft.AspNet.Server.WebListener": "1.0.0-beta8-15656"
  },

  "commands": {
